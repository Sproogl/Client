             xmlns:Itemlist="clr-namespace:Itemlist;assembly=ItemList" xmlns:FoundItemList="clr-namespace:FoundItemList;assembly=FoundItemList" x:Class="SearchFriendPanel.SearchPanelControl"
             mc:Ignorable="d" Height="426" Width="255.333">
    <Grid Background="#FFFA8E40">
        <TextBox x:Name="searchTextBox" Height="20" VerticalAlignment="Top" Margin="3,5,0,0" HorizontalAlignment="Left" Width="208" />
